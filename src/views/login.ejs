<%- include('./partials/head')  %>
    <body>
        <%- include('./partials/header')  %>
        <main>
          <form action="/login" method="post" enctype = "multipart/form-data">
            <div class="container">
              <div class="row">
                <div class="col-25">
                  <label for="fname">Usuario</label>
                </div>
                <div class="col-75">
                  <input
                    type="email"
                    id="usuario"
                    name="usuario"
                    placeholder="Email"
                  />
                </div>
              </div>
              <div class="row">
                <div class="col-25">
                  <label for="lname">Password</label>
                </div>
                <div class="col-75">
                  <input
                    type="password"
                    id="contraseña"
                    name="contraseña"
                    placeholder="Password"
                  />
                </div>
              </div>
              <div class="row">
                <input type="submit" value="Submit" />
              </div>
            </div>
          </form>
        </main>
        <%- include('./partials/footer')  %>
    </body>
</html>